{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "Paie residents",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {    
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "dialog": {
        "open": true,
        "save": true,
        "message": true,
        "confirm": true
      }
    },
    "windows": [
      {
        "title": "CHU",
        "maximized": true
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.CHU.gestionPaie",
      "icon": [
        "icons/iconCHU.ico"
      ]
    }
  }
}
